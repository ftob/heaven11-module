{
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "name": "ftob/heaven11-module",
    "description": "Heaven11 client",
    "keywords": [
        "modules",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Volkov",
            "email": "litebackend@gmail.com",
            "role": "Developer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "friendsofphp/php-cs-fixer": "^2.7",
        "laravel/framework": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Heaven11\\Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Heaven11\\Client\\Tests\\": "tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Heaven11\\Client\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
